{
  "name": "synx",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "test": "vitest run --config vitest.config.ts",
    "test:watch": "vitest --config vitest.config.ts",
    "test:frp": "pnpm --filter @synx/frp test",
    "examples:counter": "tsx examples/frp/counter.ts",
    "examples": "pnpm run examples:counter && pnpm run examples:zip",
    "examples:zip": "tsx examples/frp/zip.ts",
    "examples:theme-dropdown": "parcel examples/dom/theme-dropdown/index.html",
    "examples:theme-dropdown-component": "parcel examples/dom/theme-dropdown-component/index.html",
    "examples:todomvc": "parcel examples/todomvc/index.html",
    "examples:js-framework-benchmark": "parcel examples/js-framework-benchmark/index.html",
    "examples:js-framework-benchmark:jsx": "parcel examples/js-framework-benchmark-jsx/index.html",
    "examples:js-framework-benchmark:onbind": "parcel examples/js-framework-benchmark-onbind/index.html",
    "bench": "vitest bench --config vitest.config.ts",
    "bench:prod": "NODE_ENV=production vitest bench --config vitest.config.ts",
    "bench:scenario:prod": "NODE_ENV=production vitest bench --config vitest.config.ts packages/frp/bench/scenario.bench.ts",
    "bench:memory": "pnpm exec node --expose-gc ./node_modules/vitest/vitest.mjs bench --config vitest.config.ts packages/frp/bench/memory.bench.ts",
    "bench:memory:diag": "node --import tsx scripts/bench-memory-diagnostics.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@iconify-json/mdi": "latest",
    "@parcel/transformer-typescript-tsc": "^2.12.0",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.17",
    "parcel": "^2.12.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vitest": "^3.2.3"
  }
}
